{
  "name": "sharp-imageproxy-v2",
  "version": 2,
  "public": true,
  "alias": [
    "imgx-static.studentsgoabroad.com"
  ],
  "builds": [
    {
      "src": "src/api/handler_v2.js",
      "use": "@now/node@0.4.28",
      "config": {
        "maxLambdaSize": "25mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/api/handler_v2.js?url=$1",
      "headers": {
        "Cache-Control": "public, s-maxage=31536000, max-age=31536000, immutable",
        "Connection": "keep-alive"
      }
    }
  ]
}
